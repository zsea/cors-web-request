{
    "manifest_version": 2,
    "name": "super-web-request",
    "description": "跨域、修改ua请求插件",
    "version": "0.0.0.1",
    "icons": {
        "128": "icons/logo_128.png"
    },
    "background": { "scripts": ["background.js"]},
    "content_scripts":[
        { 
            "js": [ "content.js" ], 
            "matches": [ "*://zsea.github.io/*"], 
            "run_at": "document_end",
            "all_frames":true 
         }
    ],
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "cookies",
        "*://*/*"
    ],
    "browser_action": {
        "default_icon": "icons/logo_19.png",
        "default_title": "跨域、修改ua请求插件"
    },
    "externally_connectable":{
        "ids":["*"],
        "matches":["*://zsea.github.io/*"]
    }
}