{
    "manifest_version": 2,
    "name": "super-web-request",
    "description": "跨域、修改ua请求插件",
    "version": "0.0.0.1",
    "icons": {
        "128": "icons/logo_128.png"
    },
    "background": { "scripts": ["background.js"]},
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "cookies",
        "*://*/*"
    ],
    "browser_action": {
        "default_icon": "icons/logo_19.png",
        "default_title": "super-web-request"
    },
    "externally_connectable":{
        "ids":["*"],
        "matches":["*://zsea.github.io/*","*://*.zsea.me/*","*://zsea.me/*"]
    }
}